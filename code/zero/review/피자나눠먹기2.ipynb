    "def solution(n):\n",
    "    answer = 0\n",
    "    \n",
    "    slice = 1\n",
    "    while True:\n",
    "        if n*slice%6==0:\n",
    "            break\n",
    "        slice += 1\n",
    "    answer = n*slice//6\n",
    "    \n",
    "    return answer\n",
    "\n",
    "solution(10)"
   ]
  }
