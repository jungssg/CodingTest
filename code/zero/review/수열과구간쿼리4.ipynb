   "source": [
    "def solution(arr, queries):\n",
    "    for s, e, k in queries:\n",
    "        for i in [i for i in range(s,e+1) if i%k==0]:\n",
    "            arr[i] += 1\n",
    "    return arr\n",
    "\n",
    "solution([0, 1, 2, 4, 3],[[0, 4, 1],[0, 3, 2],[0, 3, 3]])"
   ]
  }
