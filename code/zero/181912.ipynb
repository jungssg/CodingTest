   "source": [
    "def solution(intStrs, k, s, l):\n",
    "    num = [int(n[s:s+l]) for n in intStrs]\n",
    "    return [n for n in num if n > k]\n",
    "\n",
    "solution([\"0123456789\",\"9876543210\",\"9999999999999\"], 50000,5,5)"
   ]
  }
