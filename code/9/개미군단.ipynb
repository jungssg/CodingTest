   "source": [
    "def solution(hp):\n",
    "    demage = [5,3,1]\n",
    "    ants = 0\n",
    "    for dmg in demage:\n",
    "        if hp==0:\n",
    "            break\n",
    "        ants += hp//dmg\n",
    "        hp %= dmg\n",
    "    return ants\n",
    "\n",
    "solution(23)"
   ]
  }
