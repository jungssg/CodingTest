   ],
   "source": [
    "def solution(num_list, n):\n",
    "    arr_list = []\n",
    "    for i in range(len(num_list))[::n]:\n",
    "        arr_list.append(num_list[i:i+n])\n",
    "    return arr_list\n",
    "\n",
    "solution([1, 2, 3, 4, 5, 6, 7, 8],2)"
   ]
  }
