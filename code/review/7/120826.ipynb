   "source": [
    "def solution(my_string, letter):\n",
    "    my_string = list(my_string)\n",
    "    while my_string.count(letter):\n",
    "        my_string.remove(letter)\n",
    "    return ''.join(my_string)\n",
    "\n",
    "solution(\"abcdef\", \"f\")"
   ]
  }
