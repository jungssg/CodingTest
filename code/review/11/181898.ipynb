   "source": [
    "def solution(arr, idx):\n",
    "    idx_arr= [i for i, a in enumerate(arr) if a]\n",
    "    for i in idx_arr:\n",
    "        if i>=idx:\n",
    "            return i\n",
    "    return -1\n",
    "\n",
    "solution([1, 0, 0, 1, 0, 0],4)"
   ]
