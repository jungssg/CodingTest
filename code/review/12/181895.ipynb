   ],
   "source": [
    "def solution(arr, intervals):\n",
    "    new = []\n",
    "    for i, j in intervals:\n",
    "        new.extend(arr[i:j+1])\n",
    "    return new\n",
    "    \n",
    "solution([1, 2, 3, 4, 5], [[1, 3], [0, 4]])"
   ]
  }
