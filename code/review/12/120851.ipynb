   "source": [
    "def solution(my_string):\n",
    "    sum = 0\n",
    "    for num in range(1,10):\n",
    "        cnt = my_string.count(str(num))\n",
    "        sum += num*cnt\n",
    "    return sum\n",
    "\n",
    "solution(\"aAb1B2cC34oOp\")"
   ]
