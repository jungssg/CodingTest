   "source": [
    "def solution(numbers, n):\n",
    "    sum = 0\n",
    "    for num in numbers:\n",
    "        sum+=num\n",
    "        if sum>n:\n",
    "            break\n",
    "    return sum\n",
    "\n",
    "solution([34, 5, 71, 29, 100, 34], 123)"
   ]
  }
