    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "HelloWorld!\n"
     ]
    }
   ],
   "source": [
    "str = input()\n",
    "if len(str) >= 1 and len(str) <= 1000000:\n",
    "    str = str.replace(\" \", '')\n",
    "    print(str)"
